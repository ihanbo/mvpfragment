<?xml version="1.0"?>
<globals>

	<!-- 这里声明全局变量-->
	<global id="SupportPackage" value=".support.v4" />
	<global id="FragmentLayout" value="${fragmentName}"/> 			//作为fragment的layout的命名
    <global id="FragmentName" value="${className}" /> 				//作为fragment类名
    <global id="PresenterName" value="${className}PresenterImpl"/> 	//作为presenter类名
    <global id="ContractName" value="${className}Contract" /> 		//作为contract类名
	<global id="IViewName" value="View" /> 							//作为view层接口名
	<global id="IPresenterName" value="Presenter" /> 				//作为presenter层接口名

</globals>
